      verMas={false}>
      {cundinamarca.slice(0,1).map((lotery:any)=>(
        lotery.lastDraw
        ?
        <article class="inline-grid snap-center shrink-0 m-auto relative h-[245px] rounded-3xl overflow-hidden group">
          <div class="absolute z-20 top-0 left-0 mt-40 pl-3 group-hover:-translate-x-48 duration-150 transition-transform">
            <a
            href={lotery.downloadImg}
            class="bg-colorPrimary absolute rounded-lg p-1 top-0 left-0 translate-x-[340px] translate-y-8 duration-150 hover:bg-purple-400"
            >
              <Icon.Download fill="white" size="30" title="Descargar" />
            </a>
            <h2 class=" font-bold text-xl text-white mb-2 text-ellipsis w-44 h-7 overflow-hidden whitespace-nowrap">{lotery.lotteryName}</h2>
            <div class="flex gap-3 items-center">
              <h3 class="font-bold text-base text-white top-0 bg-colorPrimary px-2 py-[2px] rounded-lg ">{lotery.drawingNumber}</h3>
              <h3 class="">
                  <p class="">
                    <time class="bg-white px-2 py-1 rounded-lg" datetime={lotery.fecha}>
                      {lotery.fecha}
                    </time>
                  </p>
                </h3>
            </div>
          </div>
          <a
            class=""
            target="_blank"
            href={lotery.img}>
          <div class="relative after:absolute after:z-10 after:top-0 after:h-full after:w-full after:content-[''] after:bg-gradient-to-t from-black/100">
              <Picture
              width={300}
              sizes="300"
              format={"webp"}
              widths={[200, 300, 400]}
              aspectRatio="16:9"
              fit="inside"
              class="bg-slate-200 w-48 h-full max-h-[245px] aspect-[9/12] object-cover opacity-70 group-hover:opacity-100 group-hover:contrast-125 transition-opacity duration-100"
              loading=""
              src={`${lotery.img}`}
              alt={lotery.imgAlt}
              title={lotery.drawingNumber}
              />
            </div>
          </a>
        </article>
        :
        null
      ))}
